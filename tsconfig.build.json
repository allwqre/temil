{
	"compilerOptions": {
		"strict": true,
		"lib": ["ESNext"],
		"module": "esnext",
		"target": "esnext",
		"moduleResolution": "bundler",
		"skipLibCheck": true,
		"declaration": true,
		"noEmit": false,
		"emitDeclarationOnly": true,
		"rootDir": "lib",
		"outDir": "dist"
	},
	"include": ["./lib/**/*"]
}
